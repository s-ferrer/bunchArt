{
  "name": "bunchart",
  "version": "0.0.1",
  "description": "Project sample Coyotiv",
  "main": "src/index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "lint-staged",
    "start": "node src/index.js",
    "prepare": "husky install"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  },
  "lint-staged": {
    "*.js": [
      "pretty-quick --staged",
      "eslint --fix"
    ],
    "*.+(json|css|md)": [
      "pretty-quick --staged"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s-ferrer/bunchart.git"
  },
  "keywords": [],
  "author": "Sara Ferrer <saraferrer@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/s-ferrer/bunchart/issues"
  },
  "homepage": "https://github.com/s-ferrer/bunchart#readme",
  "devDependencies": {
    "eslint": "^8.6.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "prettier": "2.5.1",
    "pretty-quick": "^3.1.3"
  },
  "dependencies": {
    "colors": "^1.4.0"
  }
